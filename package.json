{
  "name": "@webex/webrtc-media",
  "version": "1.16.1",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "devsupport@webex.com",
  "repository": {
    "type": "git",
    "url": "https://sqbu-github.cisco.com/WebExSquared/webrtc-media-core"
  },
  "publishConfig": {
    "registry": "https://engci-maven.cisco.com/artifactory/api/npm/webex-npm-group/",
    "tag": "latest"
  },
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "karma start --browsers chrome",
    "test:integration": "karma start --integration",
    "test:integration:debug": "karma start --integration --browsers=chrome --single-run=false --debug --auto-watch",
    "test:debug": "karma start --browsers=chrome --single-run=false --debug --auto-watch",
    "lint": " eslint 'src/**/*.ts' --fix",
    "prepare": "husky install",
    "release": "semantic-release",
    "docs": "rimraf ./docs && api-extractor run -c ./api-extractor.json && api-documenter markdown -i ./docs/temp -o ./docs",
    "docs:publish": "gh-pages-deploy"
  },
  "dependencies": {
    "events": "^3.3.0",
    "detectrtc": "^1.4.1",
    "sdp-transform": "^2.14.1",
    "uuid": "^8.3.2",
    "webrtc-adapter": "^8.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@microsoft/api-documenter": "^7.13.54",
    "@microsoft/api-extractor": "^7.18.11",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "15.14.7",
    "@types/sdp-transform": "^2.4.5",
    "@types/sinon": "^10.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "chai": "^4.3.4",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "gh-pages-deploy": "^0.5.1",
    "husky": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^4.3.6",
    "karma-typescript": "^5.5.2",
    "karma-typescript-es6-transform": "^5.5.2",
    "mocha": "^9.1.2",
    "puppeteer": "^10.4.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.7",
    "sinon": "^11.1.2",
    "typescript": "^4.3.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        [
          "lower-case",
          "pascal-case"
        ]
      ]
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "docs",
            "package.json",
            "yarn.lock"
          ]
        }
      ]
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "docs",
    "noprompt": true
  }
}
