{
  "name": "@webex/webrtc-media-core",
  "version": "0.0.0-alpha",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "devsupport@webex.com",
  "repository": {
    "type": "git",
    "url": "https://sqbu-github.cisco.com/WebExSquared/webrtc-media-core"
  },
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "wtr --coverage",
    "test:integration": "SAUCE=true wtr --group integration",
    "lint": " eslint 'src/**/*.ts' --fix",
    "prepare": "husky install",
    "release": "semantic-release",
    "docs": "rimraf ./docs && api-extractor run -c ./api-extractor.json && api-documenter markdown -i ./docs/temp -o ./docs",
    "docs:publish": "gh-pages-deploy"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@esm-bundle/chai": "*",
    "@microsoft/api-documenter": "^7.13.49",
    "@microsoft/api-extractor": "^7.18.9",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@types/chai": "^4.2.21",
    "@types/node": "15.14.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@web/dev-server-esbuild": "^0.2.14",
    "@web/test-runner": "^0.13.17",
    "@web/test-runner-junit-reporter": "^0.4.7",
    "@web/test-runner-playwright": "^0.8.8",
    "@web/test-runner-puppeteer": "^0.10.2",
    "@web/test-runner-saucelabs": "^0.6.2",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "gh-pages-deploy": "^0.5.1",
    "husky": "^7.0.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^3.4.0",
    "semantic-release": "^17.4.7",
    "typescript": "^4.3.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        [
          "lower-case",
          "pascal-case"
        ]
      ]
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "docs/",
            "package.json",
            "yarn.lock"
          ]
        }
      ]
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "docs",
    "noprompt": true
  }
}
